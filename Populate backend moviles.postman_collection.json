							"",
							"var newestUserId = pm.globals.get(\"newestUserId\");",
							"pm.globals.set(\"randomUserId\", _.random(1, newestUserId))",
							"",
							"var Courts = [\"Cancha1\", \"Cancha2\", \"Cancha3\"];",
							"pm.globals.set(\"randomCourt\", Courts[Math.floor(Math.random() * Courts.length)]);",
							"",
